        "# Função para gerar resumo do dataframe\n",
        "def summarize_df(df):\n",
        "    num_var = len(df.columns)\n",
        "    num_obs = len(df)\n",
        "    cels_faltantes = df.isnull().sum().sum()\n",
        "    linhas_duplicadas = df.duplicated().sum()\n",
        "\n",
        "    numeric_vars = len(df.select_dtypes(include=np.number).columns)\n",
        "    categorical_vars = len(df.select_dtypes(include='object').columns)\n",
        "\n",
        "    summary = {\n",
        "        'shape': df.shape,\n",
        "        'num_var': num_var,\n",
        "        'num_obs': num_obs,\n",
        "        'cels_faltantes': cels_faltantes,\n",
        "        'linhas_duplicadas': linhas_duplicadas,\n",
        "        'numeric_vars': numeric_vars,\n",
        "        'categorical_vars': categorical_vars,\n",
        "        'dtypes': df.dtypes.to_dict(),\n",
        "        'valores_faltantes': df.isnull().sum().to_dict(),\n",
        "        'duplicados': df.duplicated().sum(),\n",
        "        'status_descritiveis': df.describe().to_dict()\n",
        "    }\n",
        "    return summary\n",
        "\n",
        "# Função para converter gráficos para Base64\n",
        "def plot_64(fig):\n",
        "    buf = BytesIO()\n",
        "    fig.savefig(buf, format='png')\n",
        "    buf.seek(0)\n",
        "    img_base64 = base64.b64encode(buf.read()).decode('utf-8')\n",
        "    buf.close()\n",
        "    return img_base64\n",
        "\n",
        "# Função para gerar gráficos e retornar como Base64 com descrição detalhada\n",
        "def create_plot(df):\n",
        "    plots = []\n",
        "\n",
        "    # Histogramas para variáveis numéricas\n",
        "    for column in df.select_dtypes(include=np.number).columns:\n",
        "        fig, ax = plt.subplots()\n",
        "        sns.histplot(df[column], kde=True, ax=ax)\n",
        "        plt.title(f'Histograma de {column}')\n",
        "        img_base64 = plot_64(fig)\n",
        "        description = (f\"O histograma de {column} mostra a distribuição dessa variável numérica. \"\n",
        "                       f\"Ele oferece uma visão geral da concentração dos valores, com uma linha de densidade estimada (KDE) para indicar a suavidade da distribuição.\")\n",
        "        plots.append({'img': img_base64, 'description': description})\n",
        "        plt.close(fig)\n",
        "\n",
        "    # Gráficos de contagem para variáveis categóricas\n",
        "    for column in df.select_dtypes(include='object').columns:\n",
        "        fig, ax = plt.subplots()\n",
        "        sns.countplot(y=df[column], ax=ax)\n",
        "        plt.title(f'Countplot de {column}')\n",
        "        img_base64 = plot_64(fig)\n",
        "        description = (f\"O gráfico de contagem (countplot) para {column} exibe a frequência de cada categoria dentro dessa variável. \"\n",
        "                       f\"É uma boa maneira de visualizar a distribuição das diferentes categorias e identificar qual delas é mais comum.\")\n",
        "        plots.append({'img': img_base64, 'description': description})\n",
        "        plt.close(fig)\n",
        "\n",
        "    # Gráfico de Distribuição de Tipos de Pokémon\n",
        "    df['Type'] = df['Type'].str.split(', ')\n",
        "    all_types = df.explode('Type')\n",
        "\n",
        "    fig, ax = plt.subplots()\n",
        "    sns.countplot(data=all_types, x='Type', order=all_types['Type'].value_counts().index, hue='Type', palette=\"Set3\", ax=ax)\n",
        "    plt.title('Distribuição de Tipos de Pokémon', fontsize=16)\n",
        "    plt.xticks(rotation=45)\n",
        "    img_base64 = plot_64(fig)\n",
        "    description = (\"Este gráfico mostra a distribuição dos tipos de Pokémon no dataset. A visualização destaca quais tipos são mais comuns, \"\n",
        "                   \"o que pode indicar popularidade ou predominância de determinados tipos no universo dos Pokémon.\")\n",
        "    plots.append({'img': img_base64, 'description': description})\n",
        "    plt.close(fig)\n",
        "\n",
        "    # Gráfico de Dispersão entre Peso e Estatísticas de Combate\n",
        "    df['Weight'] = df['Weight'].str.extract('(\\d+\\.\\d+|\\d+)').astype(float)\n",
        "\n",
        "    fig, ax = plt.subplots()\n",
        "    sns.scatterplot(x='Weight', y='Attack Base', data=df, label='Attack', color='red', marker='x', ax=ax)\n",
        "    sns.scatterplot(x='Weight', y='Defense Base', data=df, label='Defense', color='blue', marker='x', ax=ax)\n",
        "    plt.title('Correlação entre Peso e Base de Ataque/Defesa', fontsize=16)\n",
        "    plt.xlabel('Peso (kg)')\n",
        "    plt.ylabel('Estatística Base')\n",
        "    plt.legend()\n",
        "    img_base64 = plot_64(fig)\n",
        "    description = (\"O gráfico de dispersão correlaciona o peso dos Pokémon com suas estatísticas de ataque e defesa. \"\n",
        "                   \"Ele ajuda a visualizar se há uma tendência clara de Pokémon mais pesados possuírem maior força de ataque ou defesa.\")\n",
        "    plots.append({'img': img_base64, 'description': description})\n",
        "    plt.close(fig)\n",
        "\n",
        "    # Gráfico de Radar para Comparação de Estatísticas Base\n",
        "    sample_pokemon = df[['Pokemon', 'HP Base', 'Attack Base', 'Defense Base', 'Special Attack Base', 'Special Defense Base', 'Speed Base']].head(3)\n",
        "\n",
        "    def create_radar_chart(df, categories):\n",
        "        N = len(categories)\n",
        "        angles = [n / float(N) * 2 * pi for n in range(N)]\n",
        "        angles += angles[:1]\n",
        "        fig, ax = plt.subplots(figsize=(6, 6), subplot_kw=dict(polar=True))\n",
